<?xml version="1.0" ?>
 <vxml version="2.0" xmlns="http://www.w3.org/2001/vxml" xml:lang="en-GB">
   <form id="demo-transfer">
     <block>
       <prompt>
         Transfer test.
       </prompt>
     </block>
     <transfer name="toto" bridge="true" dest="exec:function(STRFTIME(${EPOCH},,%R))">
     </transfer>
     <block>
       <prompt>
         End of Transfer test <value expr="toto$.value" />.
       </prompt>
     </block>
   </form>
 </vxml>

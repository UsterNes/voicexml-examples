<?xml version="1.0" ?>
 <vxml version="2.0" xmlns="http://www.w3.org/2001/vxml">
   <form id="demo-transfer">
     <block>
       <prompt>
         <prosody volume="x-loud">Transfer test.</prosody>
       </prompt>
     </block>
     <transfer name="toto" bridge="true" dest="execute:set(VXML_MOMO)=10">
     </transfer>
     <block>
       <prompt>
         End of Transfer test <value expr="toto$.value" />.
       </prompt>
     </block>
   </form>
 </vxml>
